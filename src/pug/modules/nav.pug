nav.nav.js-nav
    ul.nav__list.js-general-nav
    
    .nav__collapsed
        .nav__collapsed-btn.js-collapsed-btn Меню 
        .nav__collapsed-content 
            ul.nav__collapsed-list.js-collapsed-nav
                each i in [1,2,3,4,5,6,7,8,9]
                    li.nav__item.nav__drop-down
                        a.nav__link(href="#") Категория #{i}
                        .nav__drop-down-content
                            ul.nav__drop-down-list
                                each j in [1,2,3,4,5,6,7]
                                    li.nav__drop-down-item
                                        a.nav__link(href="#") Подкатегория #{j}

